<div class="posts">
    {{#each posts}}
    <article class="card">
        <div class="card-header">
            <h4>
                <a href="/dashboard/edit/{{id}}">{{title}}</a>
            </h4>
            <h8>
                Created on: {{format_date createdAt}}
            </h8>
        </div>
        <div class="card-body">
            {{body}}
        </div>
    </article>
    {{/each}}
</div>